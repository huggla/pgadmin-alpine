initPgadminServerMode(){
   local pw="$(makePwForUser server)"
   printInitPassword "$pw"
   finalCmdString="/usr/bin/env -i /usr/local/bin/sudo -u $VAR_LINUX_USER /usr/local/bin/python3.6 /usr/lib/python3.6/site-packages/pgadmin4/pgAdmin4.py <<-eot &\n$VAR_email_server\n$pw\n$pw\neot"
   ampAppendFinal="false"
}
